{
  "name": "filemarks",
  "displayName": "Filemarks",
  "description": "Advanced bookmark management with numbered shortcuts and folder organization",
  "version": "0.1.0",
  "publisher": "gyuha",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "bookmark",
    "bookmarks",
    "numbered bookmarks",
    "navigation",
    "filemarks"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "filemarks",
          "title": "Filemarks",
          "icon": "$(bookmark)"
        }
      ]
    },
    "views": {
      "filemarks": [
        {
          "id": "filemarks.treeView",
          "name": "Bookmarks"
        }
      ]
    },
    "commands": [
      {
        "command": "filemarks.goToBookmark",
        "title": "Filemarks: Go to Bookmark"
      },
      {
        "command": "filemarks.deleteBookmark",
        "title": "Filemarks: Delete Bookmark",
        "icon": "$(trash)"
      },
      {
        "command": "filemarks.renameBookmark",
        "title": "Filemarks: Rename Bookmark",
        "icon": "$(edit)"
      },
      {
        "command": "filemarks.list",
        "title": "Filemarks: List Bookmarks in Current File"
      },
      {
        "command": "filemarks.listAll",
        "title": "Filemarks: List All Bookmarks"
      },
      {
        "command": "filemarks.createFolder",
        "title": "Filemarks: Create Folder",
        "icon": "$(new-folder)"
      },
      {
        "command": "filemarks.deleteFolder",
        "title": "Filemarks: Delete Folder",
        "icon": "$(trash)"
      },
      {
        "command": "filemarks.renameFolder",
        "title": "Filemarks: Rename Folder",
        "icon": "$(edit)"
      },
      {
        "command": "filemarks.moveToFolder",
        "title": "Filemarks: Move to Folder",
        "icon": "$(folder)"
      },
      {
        "command": "filemarks.toggleBookmark0",
        "title": "Filemarks: Toggle Bookmark 0"
      },
      {
        "command": "filemarks.toggleBookmark1",
        "title": "Filemarks: Toggle Bookmark 1"
      },
      {
        "command": "filemarks.toggleBookmark2",
        "title": "Filemarks: Toggle Bookmark 2"
      },
      {
        "command": "filemarks.toggleBookmark3",
        "title": "Filemarks: Toggle Bookmark 3"
      },
      {
        "command": "filemarks.toggleBookmark4",
        "title": "Filemarks: Toggle Bookmark 4"
      },
      {
        "command": "filemarks.toggleBookmark5",
        "title": "Filemarks: Toggle Bookmark 5"
      },
      {
        "command": "filemarks.toggleBookmark6",
        "title": "Filemarks: Toggle Bookmark 6"
      },
      {
        "command": "filemarks.toggleBookmark7",
        "title": "Filemarks: Toggle Bookmark 7"
      },
      {
        "command": "filemarks.toggleBookmark8",
        "title": "Filemarks: Toggle Bookmark 8"
      },
      {
        "command": "filemarks.toggleBookmark9",
        "title": "Filemarks: Toggle Bookmark 9"
      },
      {
        "command": "filemarks.jumpToBookmark0",
        "title": "Filemarks: Jump to Bookmark 0"
      },
      {
        "command": "filemarks.jumpToBookmark1",
        "title": "Filemarks: Jump to Bookmark 1"
      },
      {
        "command": "filemarks.jumpToBookmark2",
        "title": "Filemarks: Jump to Bookmark 2"
      },
      {
        "command": "filemarks.jumpToBookmark3",
        "title": "Filemarks: Jump to Bookmark 3"
      },
      {
        "command": "filemarks.jumpToBookmark4",
        "title": "Filemarks: Jump to Bookmark 4"
      },
      {
        "command": "filemarks.jumpToBookmark5",
        "title": "Filemarks: Jump to Bookmark 5"
      },
      {
        "command": "filemarks.jumpToBookmark6",
        "title": "Filemarks: Jump to Bookmark 6"
      },
      {
        "command": "filemarks.jumpToBookmark7",
        "title": "Filemarks: Jump to Bookmark 7"
      },
      {
        "command": "filemarks.jumpToBookmark8",
        "title": "Filemarks: Jump to Bookmark 8"
      },
      {
        "command": "filemarks.jumpToBookmark9",
        "title": "Filemarks: Jump to Bookmark 9"
      }
    ],
    "keybindings": [
      {
        "command": "filemarks.toggleBookmark0",
        "key": "ctrl+shift+0",
        "mac": "cmd+shift+0",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark1",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark2",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark3",
        "key": "ctrl+shift+3",
        "mac": "cmd+shift+3",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark4",
        "key": "ctrl+shift+4",
        "mac": "cmd+shift+4",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark5",
        "key": "ctrl+shift+5",
        "mac": "cmd+shift+5",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark6",
        "key": "ctrl+shift+6",
        "mac": "cmd+shift+6",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark7",
        "key": "ctrl+shift+7",
        "mac": "cmd+shift+7",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark8",
        "key": "ctrl+shift+8",
        "mac": "cmd+shift+8",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.toggleBookmark9",
        "key": "ctrl+shift+9",
        "mac": "cmd+shift+9",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark0",
        "key": "ctrl+0",
        "mac": "cmd+0",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark1",
        "key": "ctrl+1",
        "mac": "cmd+1",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark2",
        "key": "ctrl+2",
        "mac": "cmd+2",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark3",
        "key": "ctrl+3",
        "mac": "cmd+3",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark4",
        "key": "ctrl+4",
        "mac": "cmd+4",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark5",
        "key": "ctrl+5",
        "mac": "cmd+5",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark6",
        "key": "ctrl+6",
        "mac": "cmd+6",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark7",
        "key": "ctrl+7",
        "mac": "cmd+7",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark8",
        "key": "ctrl+8",
        "mac": "cmd+8",
        "when": "editorTextFocus"
      },
      {
        "command": "filemarks.jumpToBookmark9",
        "key": "ctrl+9",
        "mac": "cmd+9",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Filemarks",
      "properties": {
        "filemarks.gutterIconFillColor": {
          "type": "string",
          "default": "#157EFB",
          "description": "Gutter icon fill color"
        },
        "filemarks.gutterIconNumberColor": {
          "type": "string",
          "default": "#FFFFFF",
          "description": "Gutter icon number color"
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "filemarks.createFolder",
          "when": "view == filemarks.treeView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "filemarks.goToBookmark",
          "when": "view == filemarks.treeView && viewItem == bookmark",
          "group": "navigation"
        },
        {
          "command": "filemarks.renameBookmark",
          "when": "view == filemarks.treeView && viewItem == bookmark",
          "group": "edit@1"
        },
        {
          "command": "filemarks.moveToFolder",
          "when": "view == filemarks.treeView && viewItem == bookmark",
          "group": "edit@2"
        },
        {
          "command": "filemarks.deleteBookmark",
          "when": "view == filemarks.treeView && viewItem == bookmark",
          "group": "edit@3"
        },
        {
          "command": "filemarks.renameFolder",
          "when": "view == filemarks.treeView && viewItem == folder",
          "group": "edit@1"
        },
        {
          "command": "filemarks.deleteFolder",
          "when": "view == filemarks.treeView && viewItem == folder",
          "group": "edit@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "package": "node esbuild.js --production",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  }
}
